    "browser-sync-webpack-plugin": "^2.2.2",
    "chalk": "^2.4.1",
    "concurrently": "^3.5.1",
    "dateformat": "^3.0.3",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-csscomb": "^3.0.8",
    "gulp-environments": "^0.1.2",
    "gulp-header": "^2.0.5",
    "gulp-minify": "^0.0.15",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
