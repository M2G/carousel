    "node": "^10.13.0",
    "npm": "^6.5.0"
  },
  "homepage": "https://github.com/m2g/integration#readme",
  "scripts": {
    "development": "concurrently \"gulp css-watch\" \"webpack --mode development --watch\" ",
    "build-css-dev": "gulp css --env=development",
    "build-css": "gulp css --env=production",
    "build-js-dev": "webpack --config webpack.config.js --mode development",
    "build-js": "webpack --config webpack.config.js --mode production",
    "build-dev": "npm run build-js-dev && npm run build-css-dev",
    "build": "npm run build-css && npm run build-js"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
