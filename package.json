    "gulp-sourcemaps": "^2.6.4",
    "gulp-watch": "^5.0.1",
    "log-symbols": "^2.2.0",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.2.1"
  }
